{
  "name": "biwebpro/bi-drutiny-plugin",
  "type": "library",
  "description": "Example plugin for Drutiny.",
  "keywords": ["drupal", "audit", "drush", "security", "report"],
  "authors": [
    {
      "name": "amazee.io",
      "role": "DevOps"
    }
  ],
  "autoload": {
    "psr-4": {
      "Drutiny\\Common\\": "src/",
      "DrutinyTests\\Common\\": "tests/src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.packagist.com/biitms"
    },
    {
      "packagist.org": false
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "doctrine/annotations": "^1.12@dev",
    "drutiny/drutiny": "^2.4"
  }
}
